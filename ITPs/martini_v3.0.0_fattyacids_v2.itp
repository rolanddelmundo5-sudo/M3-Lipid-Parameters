;;;;; 
;;;;; Oleic acid
;;;;; OA
;;;;; 

;;;;; BEAD-INDEX MAPPING
;;;;; topology description
;
; COO-C1A-D2A-C3A-C4A    1-2-3-4-5
;

[moleculetype]
OA 	1

[atoms]
; id type   resnr     residu   atom    cgnr charge
1   SQ5n 	1 	OA 	COO 	1      -1.0
2   C1  	1 	OA 	C1A 	2  	0 ; 5-1 mapping
3   C4h 	1 	OA 	D2A 	3  	0
4   C1  	1 	OA 	C3A 	4  	0
5   C1  	1 	OA 	C4A 	5  	0
		
[bonds]
; i j funct length force.c.
1 2 	b_COO_C1_fa_5long    
2 3 	b_C1_C4_mid_5long
3 4 	b_C1_C4_mid
4 5 	b_C1_C1_end

[angles]
 1  2  3 	a_COO_C1_C4_fa
 2  3  4 	a_C1_C4_C1_def 
 3  4  5 	a_C1_C1_C1_def

